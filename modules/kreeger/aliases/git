alias ga='git add'
alias gcl='git clone'
alias gs='git status'
alias gt='git track'
alias gc='git commit -vS'
alias glgg='git log --graph --max-count=10 --show-signature'
alias glg='git log --stat --max-count=10 --show-signature'
alias glgga='git log --graph --decorate --all --show-signature'
alias gd='git diff'
alias gp='gpo'
alias gri='git rebase -iS'
alias grid='gri HEAD~`grevs develop`'
alias grim='gri HEAD~`grevs master`'
